package org.huasi.car.system.entity;

import java.io.Serializable;
import java.util.Date;

public class SysHelper implements Serializable {

	private static final long serialVersionUID = -7349133003735255505L;

	private int helperId;// 帮助主键
	private String helperTitle;// 标题
	private String helperContent;// 内容
	private String helperImgUrl;// 帮助图片地址
	private int helperOrder;// 排序规则
	private Date createTime;// 创建时间

	public int getHelperId() {
		return helperId;
	}

	public void setHelperId(int helperId) {
		this.helperId = helperId;
	}

	public String getHelperTitle() {
		return helperTitle;
	}

	public void setHelperTitle(String helperTitle) {
		this.helperTitle = helperTitle;
	}

	public String getHelperContent() {
		return helperContent;
	}

	public void setHelperContent(String helperContent) {
		this.helperContent = helperContent;
	}

	public String getHelperImgUrl() {
		return helperImgUrl;
	}

	public void setHelperImgUrl(String helperImgUrl) {
		this.helperImgUrl = helperImgUrl;
	}

	public int getHelperOrder() {
		return helperOrder;
	}

	public void setHelperOrder(int helperOrder) {
		this.helperOrder = helperOrder;
	}

	public Date getCreateTime() {
		return createTime;
	}

	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}

}
