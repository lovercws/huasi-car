package org.huasi.car.info.entity;

import java.io.Serializable;
import java.util.Date;

public class ActiveLike implements Serializable {

	private static final long serialVersionUID = -3286690560124309497L;

	private int activeLikeId;// 动态的点赞id
	private int activeId;// 动态id
	private int userId;// 点赞用户id
	private int activeLikeStatus;// 状态 1：正常点赞；2 取消点赞 ; 3贬
	private Date activeLikeCreateTime;// 点赞时间

	public int getActiveLikeId() {
		return activeLikeId;
	}

	public void setActiveLikeId(int activeLikeId) {
		this.activeLikeId = activeLikeId;
	}

	public int getActiveId() {
		return activeId;
	}

	public void setActiveId(int activeId) {
		this.activeId = activeId;
	}

	public int getUserId() {
		return userId;
	}

	public void setUserId(int userId) {
		this.userId = userId;
	}

	public int getActiveLikeStatus() {
		return activeLikeStatus;
	}

	public void setActiveLikeStatus(int activeLikeStatus) {
		this.activeLikeStatus = activeLikeStatus;
	}

	public Date getActiveLikeCreateTime() {
		return activeLikeCreateTime;
	}

	public void setActiveLikeCreateTime(Date activeLikeCreateTime) {
		this.activeLikeCreateTime = activeLikeCreateTime;
	}

}
