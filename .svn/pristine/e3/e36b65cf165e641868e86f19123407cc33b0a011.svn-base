package org.hausi.car.jpush.exception;

/**
 * 推送 Device出现异常
 * @author ganliang
 */
public class JPushDeviceException extends Exception {

	private static final long serialVersionUID = -398457421021748850L;

	public JPushDeviceException() {
		super();
	}

	public JPushDeviceException(String e, Throwable arg1) {
		super(e, arg1);
	}

	public JPushDeviceException(String e) {
		super();
	}

	public JPushDeviceException(Throwable e) {
		super();
	}

}
