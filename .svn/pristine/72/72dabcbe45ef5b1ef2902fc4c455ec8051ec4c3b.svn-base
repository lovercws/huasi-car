package org.huasi.car.payment.wxpay.exception;

/**
 * @desc 微信令牌异常 
 * @author ganliang
 * @version 2016年8月23日 上午9:50:33
 */
public class WXPayException extends Exception {

	private static final long serialVersionUID = -913944286974857877L;

	public WXPayException() {
		super();
	}

	public WXPayException(String message, Throwable cause) {
		super(message, cause);
	}

	public WXPayException(String message) {
		super(message);
	}

	public WXPayException(Throwable cause) {
		super(cause);
	}

}
