package org.huasi.car.info.entity;

import java.io.Serializable;
import java.util.Date;

/**
 * 动态评论
 */
public class ActiveComment implements Serializable{

	private static final long serialVersionUID = -1515105298870713910L;

	private int activeCommId;// 动态评论主键
	private int activeId;// 动态主键
	private int activeClassifyId;// 动态分类id
	private int userId;// 评论用户id
	private String activeCommContent;// 评论内容
	private String activeCommImg;// 评论图片
	private int activeCommStatus;// 评论的状态 1正常；2删除
	private Date activeCommCreateTime;// 创建时间
	public int getActiveCommId() {
		return activeCommId;
	}
	public void setActiveCommId(int activeCommId) {
		this.activeCommId = activeCommId;
	}
	public int getActiveId() {
		return activeId;
	}
	public void setActiveId(int activeId) {
		this.activeId = activeId;
	}
	public int getActiveClassifyId() {
		return activeClassifyId;
	}
	public void setActiveClassifyId(int activeClassifyId) {
		this.activeClassifyId = activeClassifyId;
	}
	public int getUserId() {
		return userId;
	}
	public void setUserId(int userId) {
		this.userId = userId;
	}
	public String getActiveCommContent() {
		return activeCommContent;
	}
	public void setActiveCommContent(String activeCommContent) {
		this.activeCommContent = activeCommContent;
	}
	public String getActiveCommImg() {
		return activeCommImg;
	}
	public void setActiveCommImg(String activeCommImg) {
		this.activeCommImg = activeCommImg;
	}
	public int getActiveCommStatus() {
		return activeCommStatus;
	}
	public void setActiveCommStatus(int activeCommStatus) {
		this.activeCommStatus = activeCommStatus;
	}
	public Date getActiveCommCreateTime() {
		return activeCommCreateTime;
	}
	public void setActiveCommCreateTime(Date activeCommCreateTime) {
		this.activeCommCreateTime = activeCommCreateTime;
	}
	
	
}
